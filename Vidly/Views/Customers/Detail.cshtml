@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    @if (@Model.Birthday != null)
    {
        <li>@Model.Birthday.Month/@Model.Birthday.Day/@Model.Birthday.Year</li>
    }
    @if (!string.IsNullOrEmpty(Model.MembershipType.Name)) {
        <li>@Model.MembershipType.Name</li>
    }
</ul>

